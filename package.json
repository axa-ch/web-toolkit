    "sassdoc": "^2.1.20",
    "serve-static": "^1.11.1",
    "slick-carousel": "^1.6.0",
    "slug": "^0.9.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "svg4everybody": "^2.1.0",
    "sync-files": "^1.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
